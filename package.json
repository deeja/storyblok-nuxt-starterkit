{
  "name": "nuxtwebsite",
  "version": "1.0.0",
  "description": "A simple Nuxt.js setup to create websites with blog feature using Storyblok as CMS and Netlify to deploy it.",
  "author": "Dominik Angerer <dominikangerer1@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt --spa",
    "generate": "nuxt build --target static && nuxt export",
    "static": "nuxt build --target static && nuxt export && nuxt serve",
    "build": "nuxt build",
    "start": "nuxt start",
    "export": "nuxt export",
    "serve": "nuxt serve",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "lint": "yarn lint:js"
  },
  "dependencies": {
    "@nuxtjs/tailwindcss": "^2.0.0",
    "axios": "^0.19.2",
    "marked": "^1.1.0",
    "nuxt": "^2.13.0",
    "storyblok-nuxt": "^1.2.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^3.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.0.5"
  }
}
